{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "checkJs": true,
    "recurse": true,
    "destination": "./docs/docs/",
    // "template": "./docs/custom-template",
    "tutorials": "./docs/tutorials"
    // "readme": "./docs/readme/readme.md"
  }
}
