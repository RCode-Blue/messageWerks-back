{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs/|_old)"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "checkJs": true,
    "recurse": true,
    "destination": "./docs/documentation/",
    "tutorials": "./docs/tutorials",
    "readme": "./README.md"
  }
}
